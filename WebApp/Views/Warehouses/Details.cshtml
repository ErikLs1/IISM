@model WebApp.Models.Index.MvcDto.WarehouseMvcDto


@{
    ViewData["Title"] = "Details";
}

<h1>@Base.Resources.Common.DetailsLink</h1>

<div>
    <h4>@Base.Resources.Common.Warehouse</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.WarehouseAddress)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.WarehouseAddress)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.WarehouseStreet)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.WarehouseStreet)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.WarehouseCity)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.WarehouseCity)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.WarehouseState)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.WarehouseState)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.WarehouseCountry)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.WarehouseCountry)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.WarehousePostalCode)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.WarehousePostalCode)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.WarehouseEmail)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.WarehouseEmail)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.WarehouseCapacity)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.WarehouseCapacity)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">@Base.Resources.Common.EditLink</a> |
    <a asp-action="Index">@Base.Resources.Common.BackToList</a>
</div>
