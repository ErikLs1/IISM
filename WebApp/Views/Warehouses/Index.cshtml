@model WebApp.Models.Index.ViewModel.WarehouseViewModel


@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Warehouses.First().WarehouseAddress))
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Warehouses.First().WarehouseEmail)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Warehouses.First().WarehouseCapacity)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Warehouses) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.WarehouseAddress)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WarehouseEmail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WarehouseCapacity)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
